{
  "name": "node-rename-path",
  "version": "4.0.1",
  "description": "Node.js path renaming made easy",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/node-rename-path",
  "bugs": "https://github.com/mgenware/node-rename-path/issues",
  "keywords": [
    "path",
    "file system"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/node-rename-path.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "mocha --require source-map-support/register dist_tests/**/*.test.js",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b tests",
    "dev": "cross-env NODE_ENV=development yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run test",
    "lint": "eslint --max-warnings 0 --ext .ts src/ tests/",
    "clean": "del-cli dist && del-cli dist_tests"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.css",
    "/dist/**/*.map"
  ],
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "cross-env": "^7.0.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "mocha": "^7.0.1",
    "source-map-support": "^0.5.16",
    "typescript": "^3.7.5"
  }
}
